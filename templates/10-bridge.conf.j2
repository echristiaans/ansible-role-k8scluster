{
    "cniVersion": "{{ cni_version }}",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{ pod_cidr_base }}{{ groups['kube-workers'].index(inventory_hostname) }}.0/24"}]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}
